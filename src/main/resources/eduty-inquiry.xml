<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="edutyCorrectDataInquiryDAO" class="com.streamit.others.dao.InquiryDAOImpl" autowire="byName">
        <property name="sql">
            <value>
                <![CDATA[
                			SELECT ROW_NUMBER() OVER(ORDER BY ID ASC) AS [NO],
                			*
                			FROM DUTY_STAMP
                            where 1 = 1
							
				]]>
            </value>
        </property>
        <property name="classMapping">
            <value>com.streamit.application.dto.CorrectData</value>
        </property>
    </bean>

    <bean id="edutyCorrectDetailInquiryDAO" class="com.streamit.others.dao.InquiryDAOImpl" autowire="byName">
        <property name="sql">
            <value>
                <![CDATA[
                			SELECT SEQ AS [NO], *, CONCAT(FIRST_NAME,' ',LAST_NAME) as FULL_NAME
                			FROM DUTY_STAMP_DETAIL
                            where 1 = 1

				]]>
            </value>
        </property>
        <property name="classMapping">
            <value>com.streamit.application.dto.CorrectDetail</value>
        </property>
    </bean>

    <bean id="edutyCorrectDetailFormInquiryDAO" class="com.streamit.others.dao.InquiryDAOImpl" autowire="byName">
        <property name="sql">
            <value>
                <![CDATA[
                			SELECT SEQ AS [NO], *, CONCAT(FIRST_NAME,' ',LAST_NAME) as FULL_NAME
                			FROM DUTY_STAMP_DETAIL
                            where 1 = 1

				]]>
            </value>
        </property>
        <property name="classMapping">
            <value>com.streamit.application.dto.CorrectDetailForm</value>
        </property>
    </bean>

    <bean id="edutyCorrectDetailAddressInquiryDAO" class="com.streamit.others.dao.InquiryDAOImpl" autowire="byName">
        <property name="sql">
            <value>
                <![CDATA[
                			SELECT *
                			FROM ADDRESS
                            where 1 = 1

				]]>
            </value>
        </property>
        <property name="classMapping">
            <value>com.streamit.application.dto.CorrectDetailAddress</value>
        </property>
    </bean>

    <bean id="edutySendDataInquiryDAO" class="com.streamit.others.dao.InquiryDAOImpl" autowire="byName">
        <property name="sql">
            <value>
                <![CDATA[
                			SELECT D1.* ,
                			ROW_NUMBER() OVER(ORDER BY D1.ID ASC) AS [NO],
                			(select count(ID) from DUTY_STAMP_DETAIL where ID=D1.ID AND SEND_RD_STATUS='F') AS FAIL
                			FROM DUTY_STAMP D1
                            where 1 = 1

				]]>
            </value>
        </property>
        <property name="classMapping">
            <value>com.streamit.application.dto.SendRdData</value>
        </property>
    </bean>

    <bean id="edutySendDetailInquiryDAO" class="com.streamit.others.dao.InquiryDAOImpl" autowire="byName">
        <property name="sql">
            <value>
                <![CDATA[
                			SELECT SEQ AS [NO], *, CONCAT(FIRST_NAME,' ',LAST_NAME) as FULL_NAME
                			FROM DUTY_STAMP_DETAIL
                            where 1 = 1

				]]>
            </value>
        </property>
        <property name="classMapping">
            <value>com.streamit.application.dto.SendRdDetail</value>
        </property>
    </bean>

    <bean id="edutyInvDataInquiryDAO" class="com.streamit.others.dao.InquiryDAOImpl" autowire="byName">
